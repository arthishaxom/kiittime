{
  "cli": {
    "version": ">= 16.4.0",
    "appVersionSource": "local"
  },
  "build": {
    "development": {
      "pnpm": "10.12.1",
      "developmentClient": true,
      "distribution": "internal",
      "env": {
        "APP_VARIANT": "development"
      }
    },
    "preview": {
      "distribution": "internal",
      "env": {
        "APP_VARIANT": "preview"
      }
    },
    "production": {
      "env": {
        "ANDROID_VERSION_CODE": "15"
      },
      "android": {
        "buildType": "app-bundle",
        "credentialsSource": "local"
      }
    }
  },
  "submit": {
    "production": {}
  }
}